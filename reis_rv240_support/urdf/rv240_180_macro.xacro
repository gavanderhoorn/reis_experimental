			<child link="${prefix}link_2"/>
			<axis xyz="0 1 0"/>
			<limit lower="-2.55" upper="1" effort="0" velocity="1.36"/>
		</joint>
		<joint name="${prefix}joint_3" type="revolute">
			<origin xyz="0 0 1.25" rpy="0 0 0"/>
