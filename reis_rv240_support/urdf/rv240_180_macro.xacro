				<geometry>
					<mesh filename="package://reis_rv240_support/meshes/rv240_180/collision/link_6.stl"/>
				</geometry>
			</collision>
		</link>
		<link name="${prefix}tool0" />


		<!-- extra links: balancer and rod -->
		<link name="${prefix}kolben">
			<visual>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<geometry>
