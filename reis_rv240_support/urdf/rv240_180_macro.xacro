				<geometry>
					<mesh filename="package://reis_rv240_support/meshes/rv240_180/collision/link_2.stl"/>
				</geometry>
			</collision>
		</link>
		<link name="${prefix}link_3">
			<visual>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<geometry>
